       :url=\"d.url\"\n            @activate=\"setActiveDepartment\">\n          </department>\n\n        </div>\n      </div>\n\n      <div class=\"gr-filters\">\n        <h4 class=\"h h4\">Topics</h4>\n        <div class=\"gr-filters-flex\">\n          <div class=\"gr-filters-form w-form\">\n            <form>\n              <select id=\"Topic\" name=\"Topic\" data-name=\"Topic\" class=\"select mod-inline w-select\">\n                <option value=\"\">All Topics</option>\n                <option value=\"First\">First Choice</option>\n                <option value=\"Second\">Second Choice</option>\n                <option value=\"Third\">Third Choice</option>\n              </select>\n            </form>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"gr-feed\">\n        <div class=\"gr-feed-articles\">\n          <div class=\"gr-feed-articles-date\">\n            <h3 class=\"h h3\">Today</h3>\n            <div class=\"gr-feed-flex\">\n\n              <div class=\"gr-feed-flex-item\" v-for=\"e in feedEntries\" :key=\"e.id\">\n                <div class=\"gr-article gr-card bg-light-grey mod-co\">\n                  <div class=\"gr-article-body\">\n                    <h6 class=\"h h6\">&lt;Department&gt;</h6>\n                    <h5 class=\"h h5 mod-article-h\">{{e.title}}</h5>\n                    <p class=\"p\">{{e.summary['#text']}}</p>\n                    <a :href=\"e.link['-href']\" class=\"button mod-co w-button\">Read the story</a>\n                  </div>\n                  <div class=\"gr-article-footer\">\n                    <div class=\"gr-article-footer-date\">Updated: {{e.updated}}</div>\n                  </div>\n                </div>\n              </div>\n\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n  </div>\n</template>\n\n<script>\n\n  import Departments from './departments.js'\n  import Department from \"./Department.vue\";\n\n  export default {\n    name: 'App',\n\n    components: {\n      'department' : Department\n    },\n\n    data(){\n      return {\n        feed: {},\n        departments: Departments,\n        activeDepartment: {}\n      }\n    },\n\n    mounted() {\n\n      const RSS_URL = 'http://localhost:8090';\n\n      fetch( RSS_URL )\n          .then( response => this.feed = response.json() )\n          .then( data => this.feed = data.feed )\n    },\n\n    computed: {\n      feedUpdated: function(){\n        return this.feed ? this.feed.updated : null\n      },\n\n      feedEntries: function(){\n        return this.feed ? this.feed.entry : null\n      }\n    },\n\n    methods: {\n      setActiveDepartment(dept){\n        this.activeDepartment = dept\n      }\n    }\n  }\n\n</script>"]}]}